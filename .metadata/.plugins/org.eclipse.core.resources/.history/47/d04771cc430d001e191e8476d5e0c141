package PDV.enum;

public enum Desconto {
	 varejo(0, 5.0),
	 atacado(Integer 1, Double 15.0),
	 fator_100_200(Integer 2, Double 5.0)
	 fator_200_500(Integer 2, Double 5.0)
	 fator_500(Integer 2, Double 5.0)
	;

	private Integer codigo;

	private Double porcentagem;
	
	Desconto(Integer codigoInteger, Double porceDouble) {
		this.codigo = codigoInteger;
		this.porcentagem = porceDouble;
	}

	public Integer getCodigo() {
		return codigo;
	}

	public void setCodigo(Integer codigo) {
		this.codigo = codigo;
	}

	public Double getPorcentagem() {
		return porcentagem;
	}

	public void setPorcentagem(Double porcentagem) {
		this.porcentagem = porcentagem;
	}


}
